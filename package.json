{
  "name": "gitlab-api-ts",
  "version": "1.0.8",
  "description": "Wrapper for Gitlab API written in typescript",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/enisz/gitlab-api-ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/enisz/gitlab-api-ts.git"
  },
  "scripts": {
    "build:clear": "rimraf dist",
    "build:module": "tsc",
    "build:types": "npm-dts generate -o dist/index.d.ts",
    "build": "npm run build:clear && npm run build:module && npm run build:types",
    "start": "ts-node ./src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "gitlab",
    "api",
    "wrapper",
    "typescript"
  ],
  "author": {
    "name": "Enisz Abdalla",
    "email": "enisz87@gmail.com",
    "url": "https://github.com/enisz"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/form-data": "^2.5.0",
    "@types/node": "^17.0.23",
    "npm-dts": "^1.3.11",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "form-data": "^4.0.0"
  }
}
